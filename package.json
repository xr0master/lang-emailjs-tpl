{
  "name": "codemirror-lang-emailjs-tpl",
  "version": "0.1.0",
  "description": "EmailJS Template language support for CodeMirror",
  "private": false,
  "author": "Sergey Khomushin <xr0master@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "mocha test/test.js",
    "build": "rollup -c"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/xr0master/lang-emailjs-tpl.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "emailjs",
    "emailjs templates",
    "emailjs-com"
  ],
  "dependencies": {
    "@codemirror/language": "^6.8.0",
    "@lezer/highlight": "^1.1.6",
    "@lezer/lr": "^1.3.6"
  },
  "devDependencies": {
    "@lezer/generator": "^1.3.0",
    "mocha": "^10.2.0",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-ts": "^3.2.0",
    "typescript": "^5.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "all"
  }
}
